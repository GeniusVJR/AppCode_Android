<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tc.dream.lovecalendar" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".CalendarActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScheduleView" android:label="@string/app_name"/>
        <activity android:name=".ScheduleTypeView" android:label="@string/app_name"/>
        <activity android:name=".ScheduleInfoView" android:label="@string/app_name"/>
        <activity android:name=".ScheduleAll" android:label="@string/app_name"/>
        <activity android:name=".CalendarConvert" android:label="@string/app_name"/>
    </application>

</manifest>
